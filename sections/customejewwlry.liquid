                      <pattern id="pattern-{{ section.id }}={{forloop.index}}" patternContentUnits="objectBoundingBox" width="1" height="1">
                      <use xlink:href="#image-{{ section.id }}={{forloop.index}}" transform="translate(-0.0282016) scale(0.00240092 0.00162338)"/>
                      </pattern>
                      <image id="image-{{ section.id }}={{forloop.index}}" width="440" height="616" xlink:href="{{ product.featured_image.src | img_url: '' }}"/>
                      </defs>
                      </svg>
                  
                  </div>


                </a>
                <div class="prouct-details ptype-1">
                    <h2>{{ product.title}}</h2>
                    <p>{{ product.price | money }}</p>
                </div>
              </div>
      
      
            </li>
            {% endfor %}
            
          </ul>
        </div>
