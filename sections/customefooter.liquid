                  </ul>
                </div>
              </div>
          </div>
          <div class="col-md-5 offset-md-3">
            <div class="footer-right-menu ">
                <div class="row">
                 <div class="col-6 col-sm-6 offset-lg-1">
                  <div class="sinel-menu-item">
                    <h6>{{ section.settings.menu1 }}</h6>
                 
                    <ul class="singel-menu-item">
                      
                      {% assign footer_menu_4 = section.settings.sub4 %}
                      {% for link in linklists[footer_menu_4].links %}
                      <li><a href="{{link.url}}">{{ link.title}}</a></li>
                      {% endfor %}
                    </ul>
                </div>
                </div>
                <div class="col-6 col-sm-6 col-lg-5 col-md-6">
                
    
                  <div class="sinel-menu-item">
                    <h6>{{ section.settings.menu2 }}</h6>
                
                    <ul class="singel-menu-item">
                      {% assign footer_menu_4 = section.settings.sub5 %}
                      {% for link in linklists[footer_menu_4].links %}
                      <li><a href="{{link.url}}">{{ link.title}}</a></li>
                      {% endfor %}
                    </ul>
                  
                </div>
                </div>
                </div>
          </div> 
              {% comment %} {% endcomment %}


          </div>
        </div>
      </div>
    </div>
    <div class="footer-copy-right">
      <div class="container">
        <div class="row">
          <div class="col-12">
            <div class="text-center f-copy-text">
              <p>{{ section.settings.copy  }}</p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </footer>

  {% schema %}
    {
        "name": "Footer",
        "class": "footer",
        "settings": [
            {
                "type": "image_picker",
                "id": "img",
                "label": "Select Your Footer logo"
            },
            {
                "type": "image_picker",
                "id": "img2",
                "label": "Select footer shap"
            },
            {
                "type": "image_picker",
                "id": "img3",
                "label": "Select footer shap for proudct page"
            },
            {
                "type": "url",
                "id": "Social",
                "label": "Twitter Url",
                "info": "Please Select twitter link"
            },
            {
                "type": "url",
                "id": "Social1",
                "label": "Youtube Url",
                "info": "Please Select Youtube link"
            },
            {
                "type": "url",
                "id": "Social2",
                "label": "Instagram Url",
                "info": "Please Select Instagram link"
            },
            {
                "type": "text",
                "id": "menu1",
                "label": "Footer Menu 1",
                "default": "QUICK LINK",
                "placeholder": "QUICK LINK"
            },
            {
                "type": "link_list",
                "id": "sub4",
                "label": "Select Footer Menu 1",
                "info": "Footer menu"
            },
            {
                "type": "text",
                "id": "menu2",
                "label": "Footer Menu 1",
                "default": "QUICK LINK",
                "placeholder": "QUICK LINK"
            },
            {
                "type": "link_list",
                "id": "sub5",
                "label": "Select Footer Menu 1",
                "info": "Footer menu"
            },
            {
                "type": "text",
                "id": "copy",
                "label": "Copy Right Text",
                "default": "© 2022 WE ARE CHIMMI",
                "info": "© 2022 WE ARE CHIMMI",
                "placeholder": "© 2022 WE ARE CHIMMI"
            }
        ]
    }
  {% endschema %}
  